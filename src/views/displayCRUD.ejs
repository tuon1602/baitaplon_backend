<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Get Crud</title>
</head>
<body>
   <table>
       <tr>
           <th>Email</th>
           <th>FirstName</th>
           <th>LastName</th>
           <th>address</th>
           <th>Action</th>
       </tr>
       <tr>
        <% for(let i=0; i < dataTable.length; i++) { %>
            <tr>
              <td><%= dataTable[i].email %></td>
              <td><%= dataTable[i].firstName %></td>
              <td><%= dataTable[i].lastName %></td>
              <td><%= dataTable[i].address %></td>
              <td>
                  <a href="/edit-crud?id=<%= dataTable[i].id %> " type="button">Edit</a>
                  <a href="" type="button">Delete</a>
              </td>
            </tr>
         <% } %>
       </tr>
   </table>
</body>
</html>